# .env.example
# Copie este arquivo para .env e preencha com seus valores

# VTEX API
VTEX_ACCOUNT_NAME=
VTEX_ENVIRONMENT=vtexcommercestable.com.br
VTEX_APP_KEY=
VTEX_APP_TOKEN=

# Bisturi API (opcional)
BISTURI_BASE=https://api.bisturi.com.br

# GitHub Token (para publicar releases)
# Criar em: https://github.com/settings/tokens?type=beta
# Permissões: Contents (Read and write)
GH_TOKEN=

# Configurações de processamento (opcional)
CONCURRENCY=4
REQUEST_DELAY_MS=200